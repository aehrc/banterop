{
  "name": "flipproxy-demo",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bun --watch src/server/index.ts",
    "start": "bun src/server/index.ts",
    "test": "bun test",
    "test:timeout": "timeout ${TEST_TIMEOUT:-5}s bun test --bail || ec=$?; if [ $ec -eq 124 ]; then echo 'Test run timed out'; exit 1; else exit $ec; fi",
    "typecheck": "bun x tsc --noEmit"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.17.4",
    "bun-storage": "^1.1.1",
    "bun-types": "^1.2.20",
    "hono": "^4.9.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "zod": "3.23.8",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@types/react-dom": "^18"
  }
}
